<style>
    .mm_menu_li{cursor:pointer}
    .mm_menu_li .mm_submenu{display:none;position:absolute;width:200px;padding-top:18px}
    .mm_menu_li:hover .mm_submenu{display:block}
    .mm_menu_li .mm_submenu li{border-bottom:1px solid #fff;background:#272525}
    .mm_menu_li .mm_submenu li:last-child{border:0}
    .mm_menu_li .mm_submenu li a{color:#fff;font-weight:400;padding:10px 20px;display:block}
    .icon-arrow{width:14px}
    .d-none{display:none}
    .mm_search_mobile{width:auto}
    @media screen and (max-width: 767px){
        .icon-arrow{transition:all .5s ease-in-out}
        .mm_menu_li.active .icon-arrow{transform:rotate(180deg);transition:all .5s ease-in-out}
        .mm_menu_li .mm_submenu{width:100%;position:relative;}
        .mm_menu_li:hover .mm_submenu{display:none}
        .mm_menu_li.active .mm_submenu{display:block}
        .mm_menu_li .mm_submenu{padding-top:0;padding-left: 50px;}
        .mm_menu_li .mm_submenu li{background: transparent;list-style:square;border:none}
        .mm_menu_li .mm_submenu li a{font-size:14px; color:inherit;padding:5px 10px}
    }
</style>
<style>@media screen and (max-width:767px){.sub_tags{display: flex;overflow-x: scroll;margin: 0 8px;}}.sub_tags{margin-bottom: 20px}</style>
<header class="header u-sizeFullWidth u-fontSizeSmaller u-bgheader u-flexCenter">
    <div class="header-wrap u-flexCenter container u-flex1">
        {{!-- Logo --}}
        <div class="header-left u-flex0">
            {{!-- <a href="{{@site.url}}" --}}
            <a href="https://manmatters.com/" target="_blank"
                class="header-logo u-block u-lineHeight1 godo-tracking"
                data-event-category="Header"
                data-event-action="Logo"
                data-event-label="Click"
                data-event-non-interaction="true">
                {{#if @site.logo}}
                    <img src="{{img_url @site.logo size="m"}}" alt="{{@site.title}}"/>
                {{else}}
                    <span class="u-fontSize28 u-fontWeightBold">{{@site.title}}</span>
                {{/if}}
            </a>
        </div>

        {{!-- Navigation --}}
        <nav style="overflow:visible!important" class="header-center u-relative mainMenu u-overflowHidden u-flex u-flex1 u-paddingRight20 u-flexCenter u-hide-before-lg u-menu-color">
