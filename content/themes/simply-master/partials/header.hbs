<script>
    window.addEventListener('DOMContentLoaded', function() {
        var pathname = window.location.pathname;
        var pathArray = pathname.split('/');
        var url_last = pathArray[pathArray.length-2];
        var cat = '';
        var tags = '';

        var health_360 = {"360-health":"360-health", "sex":"Sex", "erectile-dysfunction":"Erectile Dysfunction", "masturbation":"Masturbation", "testosterone":"Testosterone", "nightfall":"Nightfall", "gym":"Gym", "weight-loss":"Weight Loss", "diet":"Diet", "hygiene":"Hygiene", "diseases":"Diseases", "sleep":"Sleep", "yoga":"Yoga", "mental-health":"Mental Health", "lifestyle":"Lifestyle"}
        jQuery.each( health_360, function(link, title) {
            if(url_last==link){
                cat = '360-health';
                tags = health_360;
            }
        });

        var skin = {"skin":"Skin", "skin-types":"Skin Types", "skin-conditions":"Skin Conditions", "skin-care-routine":"Skin Care Routine", "ingredients": "Ingredients", "foods-for-skin":"Foods for Skin", "face-packs":"Face Packs", "skin-care-tips":"Skin Care Tips", "bath-products":"Bath Products"}
        jQuery.each( skin, function(link, title) {
            if(url_last==link){
                cat = 'skin';
                tags = skin;
            }
        });

        var hair = {"hair":"Hair", "hair-regrowth":"Hair Regrowth", "hair-loss":"Hair Loss", "dandruff":"Dandruff", "hair-care":"Hair Care", "hair-masks": "Hair Masks", "grey-hair":"Grey Hair", "beard":"Beard", "hair-removal":"Hair Removal"}
        jQuery.each( hair, function(link, title) {
            if(url_last==link){
                cat = 'hair';
                tags = hair;
            }
        });

        var ayurveda = {"ayurveda":"Ayurveda", "ayurvedic-herbs":"Ayurvedic Herbs", "ayurvedic-oils":"Ayurvedic Oils", "ayurvedic-treatment":"Ayurvedic Treatment"}
        jQuery.each( ayurveda, function(link, title) {
            if(url_last==link){
                cat = 'ayurveda';
                tags = ayurveda;
            }
        });

        var home_remedies = {"home-remedies":"Home Remedies", "all-remedies":"All Remedies", "dandruff-remedies":"Dandruff Remedies", "clear-skin-remedies":"Clear Skin Remedies", "weight-loss-remedies":"Weight Loss Remedies", "hair-remedies":"Hair Remedies"}
        jQuery.each( home_remedies, function(link, title) {
            if(url_last==link){
                cat = 'home-remedies';
                tags = home_remedies;
            }
        });

        var diets = {"diets":"Diets", "hair-diets":"Hair Diets", "weight-loss-diets":"Weight loss Diets", "clear-skin-diets":"Clear skin diets", "increase-libido-diets":"Increase Libido Diets", "gym-diets":"Gym Diets"}
        jQuery.each( diets, function(link, title) {
            if(url_last==link){
                cat = 'diets';
                tags = diets;
            }
        });

        jQuery.each( tags, function( link, title ) {
            $( ".sub_tags" )
            .append( '<a href="/blog/'+cat+'/'+link+'" title="'+title+'" id="'+link+'" class="u-textCapitalize button button--outline-success">'+title+'</a>' );
            if(link == url_last){
                $("#"+link).css({"background-color":"#28a745", "border-color":"#28a745", "color":"#fff"});
                // $(".sub_tags").scrollTo("#"+link);
                $('.sub_tags').animate({
                    scrollLeft: $("#"+link).offset().left
                }, 200);
            }
        });
    });
</script>